{
  "name": "@contracts/canvas",
  "version": "1.0.0",
  "license": "AGPL-3.0-only",
  "private": true,
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "hardhat clean",
    "compile": "hardhat compile",
    "block-number": "hardhat block-number",
    "test": "hardhat test",
    "account": "hardhat accounts",
    "format:check": "prettier . --check --cache --cache-location=\".cache/prettiercache\"",
    "format:cmd": "prettier --check --cache --cache-location=\".cache/prettiercache\"",
    "format:fix": "prettier . --write --cache --cache-location=\".cache/prettiercache\"",
    "format:solidity": "prettier --write --plugin=prettier-plugin-solidity 'contracts/**/*.sol' --cache --cache-location=\".cache/prettiercache\""
  },
  "dependencies": {
    "@chainlink/contracts": "^0.6.1",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^5.7.2",
    "express": "^4.18.2",
    "ts-node": "^10.9.1",
    "typechain": "^8.2.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^2.0.1",
    "@nomicfoundation/hardhat-verify": "^2.0.7",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "hardhat": "^2.12.6",
    "prettier": "^3.2.5",
    "prettier-plugin-solidity": "^1.3.1"
  },
  "packageManager": "yarn@4.7.0"
}
